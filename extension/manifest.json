{
  "manifest_version": 2,
  "name": "StudyCat",
  "description": "A Chrome extension that helps you procrastinate less by gamifying productivity",
  "version": "0.1",
  "browser_action": {
    "default_popup": "main.html"
  },
  "content_scripts": [{
    "matches": [
      "http://*",
      "https://*"
    ],
    "js": ["import/jquery.min.js", "main.js"]
  }]
}
